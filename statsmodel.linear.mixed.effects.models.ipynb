   "source": [
    "import statsmodels.api as sm\n",
    "import statsmodels.formula.api as smf"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "097653eb",
   "metadata": {},
   "outputs": [],
   "source": [
    "data = sm.datasets.get_rdataset(\"dietox\", \"geepack\").data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "dbb04988",
   "metadata": {},
   "outputs": [],
   "source": [
    "md = smf.mixedlm(\"Weight ~ Time\", data, groups=data[\"Pig\"])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "1e0c6b46",
   "metadata": {},
   "outputs": [],
   "source": [
    "mdf = md.fit()"
   ]
